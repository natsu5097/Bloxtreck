{
  "name": "bloxtreck-wiki",
  "version": "2.0.0",
  "description": "Ultimate Blox Fruits Wiki and Guide Hub",
  "main": "index.html",
  "scripts": {
    "dev": "python -m http.server 8000",
    "build": "npm run minify:css && npm run minify:js",
    "minify:css": "cleancss -o style.min.css style.css",
    "minify:js": "uglifyjs script.js -o script.min.js -c -m",
    "lint:html": "htmlhint **/*.html",
    "lint:css": "stylelint **/*.css",
    "lint:js": "eslint **/*.js",
    "test:a11y": "pa11y-ci --sitemap https://natsu5097.github.io/Bloxtreck/sitemap.xml",
    "test:performance": "lighthouse-ci autorun",
    "optimize:images": "imagemin assets/images/**/* --out-dir=assets/images/optimized/",
    "validate:html": "html-validate **/*.html",
    "validate:links": "hyperlink index.html -r",
    "audit": "npm run lint:html && npm run lint:css && npm run lint:js && npm run test:a11y"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.50.0",
    "html-validate": "^8.0.0",
    "htmlhint": "^1.1.4",
    "hyperlink": "^5.0.4",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "pa11y-ci": "^3.1.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "uglify-js": "^3.17.4"
  },
  "keywords": [
    "blox-fruits",
    "roblox",
    "wiki",
    "gaming",
    "guide"
  ],
  "author": "Natsu",
  "license": "MIT",
  "homepage": "https://natsu5097.github.io/Bloxtreck/",
  "repository": {
    "type": "git",
    "url": "https://github.com/natsu5097/Bloxtreck.git"
  }
}